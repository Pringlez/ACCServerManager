<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <groupId>org.accmanager</groupId>
    <artifactId>acc-manager</artifactId>
    <version>0.5.0</version>
    <packaging>pom</packaging>

    <name>ACC Manager :: Parent</name>
    <description>The parent pom for the acc manager service</description>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <linkXRef>false</linkXRef>
        <input.yaml.file>acc-manager.yaml</input.yaml.file>
        <java.version>11</java.version>
        <maven.compiler.target>11</maven.compiler.target>
        <maven.compiler.source>11</maven.compiler.source>
        <servlet.api.version>2.5</servlet.api.version>
        <node.version>v16.16.0</node.version>
        <npm.version>8.11.0</npm.version>
        <spring.framework.version>5.3.22</spring.framework.version>
        <spring.boot.version>2.7.3</spring.boot.version>
        <openapi.generator.version>5.4.0</openapi.generator.version>
        <frontend.maven.plugin.version>1.12.0</frontend.maven.plugin.version>
        <aspectj.maven.plugin.version>1.12.6</aspectj.maven.plugin.version>
        <maven.resources.plugin.version>2.6</maven.resources.plugin.version>
        <jackson.databind.version>2.13.4</jackson.databind.version>
        <h2.version>2.1.214</h2.version>
        <validation.api.version>2.0.1.Final</validation.api.version>
        <docker.java.version>3.2.13</docker.java.version>
        <cucumber.spring.version>7.6.0</cucumber.spring.version>
        <rest.assured.version>5.1.1</rest.assured.version>
        <jackson.databind.nullable.version>0.2.2</jackson.databind.nullable.version>
        <maven.source.plugin.version>3.2.1</maven.source.plugin.version>
        <maven.jar.plugin.version>3.2.2</maven.jar.plugin.version>
        <maven.compiler.plugin.version>3.10.1</maven.compiler.plugin.version>
        <junit-platform.version>1.9.0</junit-platform.version>
        <junit.jupiter.version>5.8.2</junit.jupiter.version>
        <maven.surefire.version>3.0.0-M7</maven.surefire.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-parent</artifactId>
            <version>${spring.boot.version}</version>
            <type>pom</type>
        </dependency>
    </dependencies>

    <modules>
        <module>api</module>
        <module>service</module>
    </modules>

</project>
